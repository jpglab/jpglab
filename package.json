{
    "name": "@jpglab/fuse",
    "version": "0.0.1",
    "type": "module",
    "description": "Connect to & control your camera via PTP with 2 lines of TypeScript",
    "exports": {
        "./web": {
            "types": "./dist/web.d.ts",
            "import": "./dist/web.mjs",
            "require": "./dist/web.js"
        },
        "./node": {
            "types": "./dist/node.d.ts",
            "import": "./dist/node.mjs",
            "require": "./dist/node.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "compile": "tsc --noEmit",
        "build": "vite build && tsc --emitDeclarationOnly",
        "test": "vitest run",
        "lint": "eslint . --ext .ts,.tsx",
        "format": "prettier --write .",
        "clean": "rm -rf dist",
        "all": "npm run compile && npm run lint && npm run format && npm run test && npm run build",
        "example:web": "echo '\nðŸ“¸ Camera Example: http://localhost:5173/examples/web/\n' && vite"
    },
    "keywords": [
        "ptp",
        "picture-transfer-protocol",
        "camera",
        "photography",
        "usb",
        "webusb",
        "nodejs",
        "iso15740"
    ],
    "author": "JPGLab",
    "license": "MIT",
    "dependencies": {
        "usb": "^2.11.0"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.13",
        "@tailwindcss/vite": "^4.1.13",
        "@types/glob": "^9.0.0",
        "@types/node": "^20.11.0",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/usb": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.2.0",
        "@vitest/ui": "^1.2.0",
        "autoprefixer": "^10.4.21",
        "buffer": "^6.0.3",
        "eslint": "^8.56.0",
        "glob": "^11.0.3",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "process": "^0.11.10",
        "react": "^19",
        "react-dom": "^19",
        "string-similarity": "^4.0.4",
        "tailwindcss": "^4.1.13",
        "typescript": "^5.9.2",
        "vite": "^7.1.7",
        "vitest": "^1.2.0"
    },
    "optionalDependencies": {},
    "engines": {
        "node": ">=18"
    }
}
